{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Labaiik-Search-Api",
      "runtimeExecutable": "npm",
      "runtimeArgs": ["run", "start:debug", "--", "--inspect-brk"],
      "console": "integratedTerminal",
      "restart": true,
      "protocol": "auto",
      "port": 9229,
      "autoAttachChildProcesses": true,
      "cwd": "${workspaceFolder}",
      "presentation": {
        "hidden": false
      },
      "outputCapture": "std"
    }
  ]
}
